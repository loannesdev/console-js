{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "console-js",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "src",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "Console JS",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/js/32x32.png",
      "icons/js/128x128.png",
      "icons/js/128x128@2x.png",
      "icons/js/icon.ico"
    ]
  }
}